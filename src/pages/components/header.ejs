<header class="default-header">
    <div id="header-side-menu-container">
        <i id="header-side-menu-icon" class="fa-solid fa-bars"></i>
    </div>
    
    <div id="header-online-infos">
        <i class="fa-regular fa-user"></i>
        <p>Online 0</p>
    </div>

    <div id="header-title-container">
        <h1>WebChat</h1>
    </div>

    <div id="header-user-infos">
        <div class="profile-circle"><%= name.slice(0, 2).toUpperCase(); %></div>
        <p><%= name; %></p>
        <a class="invisible-link" href="./logout/"><i id="header-icon-logout" class="fa-solid fa-arrow-right-from-bracket"></i></a>
    </div>
</header>